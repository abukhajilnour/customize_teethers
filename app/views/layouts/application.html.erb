<!DOCTYPE html>
<html>
  <head>
    <title>CustomisedTeethersAndPacifierclipHolder</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
    <%= favicon_link_tag 'fav.ico' %>
    <meta name="description" content="twinkle dreams vancouver">
    <meta name="keywords" content="vancouver teether">
    <meta name="author" content="customized teether">
    <meta content="baby pacifierclip">
    <meta content="canada teether">
    <meta content="baby name teether">
  </head>
  <body>
  <div class="header-top">
    <div class="wrap"> 
      <p><%= image_tag "2.png" %> FREE SHIPPING on orders over CAD $150</p>
      <div class="clear"></div>
    </div>
  </div>
  <div class="header-bottom">
    <div class="wrap">
    <div class="header-bottom-left">
      <div class="logo">
        <%= link_to root_path do %><%= image_tag "twinkledreamslogo.png" %><% end %>
      </div>
      <div class="menu">
        <ul class="megamenu skyblue">
          <li class="active grid"><%= link_to "Home",root_path %></li>  
          <li class="drop-down"><%= link_to "Teethers",teethers_path %>
            <div class="dropdown-content">
              <% Type.all.each do |type| %>
                <p><%=link_to type.name,type %></p><hr>
              <% end %>
            </div>
          </li>
        <li><%= link_to "Baby", baby_path %></li>
        <li><%= link_to "reviews", reviews_path %></li>        
        <li><%= link_to "Contact",new_contact_path,class: "color7"%></li>
      </ul>
      </div>
    </div>
    
     <div class="header-bottom-right">
         <div class="search"> 
         <%= form_for "",url: teethers_path, role: "search", method: :get  ,class: "textbox" do %>
         <%= text_field_tag :search, @search_term,value: "Search", onfocus: "this.value = '';" ,onblur: "if (this.value == '') {this.value = 'Search';}" %>
              <input type="submit" value="Subscribe" id="submit" name="submit">
        <% end %>  
        
   
        <div id="response"> </div>
     </div>

    </div>
     <div class="clear"></div>
     </div>
  </div>  </div>
    <%= yield %>
<div class="clear"></div>
      </div>
       <div class="clear"></div>
       </div>
    </div>

  <div class="footer">
    <div class="footer-top">
      <div class="wrap">
        <div class="section group example">
        <div class="col_1_of_2 span_1_of_2">
          <ul class="f-list">
            <li><%= image_tag "2.png", width: "80"%><span class="f-text">Free Shipping on orders over $150</span><div class="clear"></div></li>
          </ul>
        </div>
        <div class="col_1_of_2 span_1_of_2">
          <ul class="f-list">
